{
  "title": "Wispr Flow Auto-Throttle",
  "rules": [
    {
      "description": "Unfreeze Wispr Flow on fn press",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "apple_vendor_keyboard_key_code": "function"
          },
          "to": [
            { "apple_vendor_keyboard_key_code": "function" },
            { "shell_command": "$HOME/Library/Scripts/WisprThrottle/unfreeze-wispr.sh" }
          ]
        }
      ]
    }
  ]
}
